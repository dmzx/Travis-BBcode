{
	"name": "dmzx/travisbbcode",
	"type": "phpbb-extension",
	"description": "This Extension will add a new BBcode for Travis Build Status.",
	"homepage": "http://www.dmzx-web.net",
	"version": "1.0.0",
	"time": "2015-11-22",
	"keywords": ["phpbb", "extension", "travisbbcode"],
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "dmzx",
			"homepage": "http://www.dmzx-web.net",
			"email": "info@dmzx-web.net",
			"role": "Extension Developer"
		}
	],
	"require": {
		"php": ">=5.3.3"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
	"extra": {
		"display-name": "Travis BBcode",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.2,<3.2.*@dev"
		},
		"version-check": {
			"host": "www.dmzx-web.net",
			"directory": "/versions",
			"filename": "travisbbcode_version.json"
		}
	}
}
